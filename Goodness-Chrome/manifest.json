{
  "manifest_version": 2,
	"name": "Goodness",
	"description": "Goodness is a customizable filter that helps you be a better person.",
	"version": "0.3.9.1",
	"background": 
  {
    "scripts": ["scripts/background-script.js"],
    "persistent": true
  },
	"options_page": "pages/options.html",
	"icons": {
		"128": "images/logo.png",
		"16": "images/logo-16.png"
	},
	"permissions": [
		"tabs",
		"<all_urls>"
	]
}
